{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "rootDir": "./",
      "moduleResolution": "Node",
      "outDir": "dist",
      "importsNotUsedAsValues": "remove",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": [
        "node"
      ]
    }
  }